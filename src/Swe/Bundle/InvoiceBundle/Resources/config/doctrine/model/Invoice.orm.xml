<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity repository-class="Swe\Compenent\Invoice\Repository\InvoiceRepository" name="Swe\Compenent\Invoice\Model\Invoice">
        <id name="id" type="integer" column="id">
            <generator strategy="AUTO"/>
        </id>
        <field name="dateInvoicing" type="date" column="dateInvoicing"/>
        <field name="designation" type="string" column="designation" length="255"/>
        <field name="amount" type="decimal" column="amount" precision="10" scale="2"/>
        <many-to-one
                field="patient"
                target-entity="Swe\Compenent\Core\Model\PatientInterface"
                join-column="patient">
            <join-column name="patient_id" referenced-column-name="id" />
        </many-to-one>
    </entity>
</doctrine-mapping>